{
  "name": "Long Division",
  "description": "Teaching long division",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "service_worker": "app.js"
  },
  
  "permissions": [
    "storage"
  ],
  "page_action": {
    "default_icon": "/icons/icon16x16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "/icons/icon16x16.png",
    "48": "/icons/icon48x48.png",
    "128": "/icons/icon128x128.png"
  }
}
